<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/constraind"
    tools:context=".User.UserAmbulance"
    android:layout_height="match_parent">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        tools:ignore="FragmentTagUsage" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/curve"
        android:backgroundTint="#F60404"
        android:padding="5dp"
        android:src="@drawable/phone"
        android:tint="@color/white"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.154"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="0.976"
        tools:ignore="ContentDescription,UseAppTint" />

    <TextView
        android:id="@+id/callnum"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:background="@drawable/curve"
        android:backgroundTint="#F60404"
        android:gravity="center"
        android:padding="15dp"
        android:text="Call 108"
        android:textColor="@color/white"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.186"
        map:layout_constraintStart_toEndOf="@+id/imageView"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="0.976"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/myview"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="28dp"
        android:textColor="@color/white"
        android:background="@drawable/curve"
        android:backgroundTint="#F60404"
        android:gravity="center"
        android:padding="15dp"
        android:text="Book"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="0.976"
        tools:ignore="HardcodedText" />


</androidx.constraintlayout.widget.ConstraintLayout>